        <!-- Blog Section Start -->
        <div class="blog-section section mt-90 mb-50" >
            <div class="container">
                <div class="row row-40">

                    <div class="col-lg-8 col-12 order-1 order-lg-2 mb-40">

                      <div class="row">

                      {% for x in forums %}
                      <div class="col-12 mb-40">
                        <div class="ee-blog">

                          <a href="{{ path('forum_show', {'id': x.id}) }}" class="image"><img src="../images/bg-01.jpg" alt="Blog Image"></a>
                          <div class="content">
                            <h3><a href="{{ path('forum_show', {'id': x.id}) }}">{{ x.titre }}</a></h3>
                            <ul class="meta">
                              <li><a href="#">{{ x.Categorie.nom }}</a></li>
                              {% set dateTime =date(x.date) %}
                              <li><a href="#">{{ dateTime|date('d') }} &nbsp; , {{ dateTime|date('F') }} &nbsp;  {{ dateTime|date('Y') }}</a></li>
                              <li><a href="#">  {{ x.nbvue }} Vue</a></li>
                              <li><a href="#">  {{ x.nblike }} Like</a></li>

                            </ul>
                            <p>{{ x.descrption }}</p>

                          </div>
                        </div>
                      </div>
                    {% endfor %}
                  </div>

              </div>

                    <div class="col-lg-4 col-12 order-2 order-lg-1">

                        <!-- Blog Sidebar -->

                           <div class="blog-sidebar mb-40">

                                <div class="product-showing">
                                    <p>Showing</p>
                                    <select name="showing" class="nice-select" onchange="Showing(this.options[this.selectedIndex].value);">
                                        <option value="6">6</option>
                                        <option value="10">10</option>
                                        <option value="14">14</option>
                                        <option value="18">18</option>
                                    </select>
                                </div>


                           </div>
